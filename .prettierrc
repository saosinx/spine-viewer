{
	"arrowParens": "avoid",
	"bracketSpacing": true,
	"endOfLine": "auto",
	"htmlWhitespaceSensitivity": "css",
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"printWidth": 100,
	"proseWrap": "preserve",
	"useTabs": true,
	"tabWidth": 2,
	"trailingComma": "es5",
	"semi": false,
	"singleQuote": true,
	"disableLanguages": [
		"vue",
		"json"
	]
}